(
  metadata.event_type = "PROCESS_LAUNCH" and
  (
    re.regex(target.process.file.full_path, `smbtouch`) nocase or
    re.regex(target.process.command_line, `smbtouch`) nocase
  )
)
or
(
  metadata.event_type = "NETWORK_CONNECTION" and
  target.port = 445 and
  (
    network.received_bytes < 1000 and
    network.sent_bytes < 1000 and
    re.regex(metadata.product_name, `IDS|IPS|Firewall`) nocase and
    re.regex(metadata.description, `EternalBlue|SMBTouch|MS17-010`) nocase
  )
)
